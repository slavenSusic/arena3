---
import PortfolioLayout from "../../layouts/PortfolioLayout.astro";
import Portfolio from "../../components/portfolio/Portfolio.jsx";

import { languages } from '../../i18n/ui';
import { getLangFromUrl, useTranslations, useTranslatedPath } from '../../i18n/utils';

const lang = getLangFromUrl(Astro.url);
const t = useTranslations(lang);
const translatePath = useTranslatedPath(lang);
---

<PortfolioLayout>
<Portfolio client:load lang={lang} headline={t('portfolio.headline')}
subHeadline={t('portfolio.subHeadline')}
apps={t('portfolio.apps')}
    miss={t('portfolio.Miscellaneous')}
    energetics={t('footer.energetika')}  banksTitle={t('banks.title')}

    banks1={t('Banks.1')}
  
    banks2={t("Banks.2")}
    banks3={t("Banks.3")}
    banks4={t("Banks.4")}
    banks5={t("Banks.5")}
    banks6={t("Banks.6")}
    
    />
</PortfolioLayout>



